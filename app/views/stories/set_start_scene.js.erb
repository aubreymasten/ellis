<% if @previous %>
  $("#scene-<%= @previous.id%>")
  .after("<%= j render partial: 'scenes/card', locals: { scene: @previous, story: @scene.story }%>")
  .remove();
<% end %>

$("#scene-<%= @story.start_scene_id%>")
.after("<%= j render partial: 'scenes/card', locals: { scene: @scene, story: @scene.story }%>")
.remove();
